

#include <stdio.h>

int sum()
{
   int n,sum=0;
   printf("Enter the size of an array: ");
   scanf("%d",&n);
   int a[n];
   for (int i=0;i<n;i++)
   {
      scanf("%d",&a[i]); 
   }
   for (int i=0;i<n;i++)
   {
     sum+=a[i];  
   }
   return sum;
}
int main()
{ 
    
    printf("sum is %d",sum());

    return 0;
}
